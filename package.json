{
  "name": "docusaurus-plugin-typedoc-api",
  "version": "1.0.0-alpha.0",
  "description": "Docusaurus plugin that provides source code API documentation powered by TypeDoc. ",
  "keywords": [
    "docusaurus",
    "plugin",
    "typedoc",
    "api"
  ],
  "scripts": {
    "check": "yarn run type && yarn run test && yarn run lint",
    "clean": "packemon clean",
    "docs": "cd website && yarn run start",
    "build": "beemo babel src/ --out-dir lib/",
    "build2": "packemon build --addEngines",
    "pack": "NODE_ENV=production packemon pack --addEngines --declaration=standard",
    "test": "beemo jest",
    "coverage": "yarn run test --coverage",
    "format": "beemo prettier",
    "lint": "beemo eslint",
    "type": "beemo typescript --project tsconfig.eslint.json",
    "release": "yarn run pack && yarn run check && npx np --yolo"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:milesj/docusaurus-plugin-typedoc-api.git"
  },
  "author": "Miles Johnson",
  "license": "MIT",
  "main": "./lib/index.js",
  "files": [
    "lib/**/*.{js,map}",
    "src/**/*.{ts,tsx,json}"
  ],
  "engines": {
    "node": ">=12.17.0",
    "npm": ">=6.13.0"
  },
  "funding": {
    "type": "ko-fi",
    "url": "https://ko-fi.com/milesjohnson"
  },
  "packemon": {
    "format": "lib",
    "platform": "node"
  },
  "peerDependencies": {
    "react": ">=16.0.0",
    "typescript": "^4.0.0"
  },
  "dependencies": {
    "@docusaurus/types": "^2.0.0-beta.3",
    "@vscode/codicons": "^0.0.21",
    "clsx": "^1.1.1",
    "typedoc": "^0.21.4"
  },
  "devDependencies": {
    "@beemo/cli": "^2.0.1",
    "@beemo/core": "^2.0.1",
    "@beemo/dev": "^1.3.1",
    "@docusaurus/module-type-aliases": "^2.0.0-beta.3",
    "@types/react": "^17.0.15",
    "packemon": "^1.2.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "typescript": "^4.3.5"
  },
  "workspaces": [
    ".",
    "./website"
  ]
}
