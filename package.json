{
  "private": true,
  "name": "docusaurus-plugin-typedoc-api-root",
  "scripts": {
    "check": "yarn run type && yarn run test && yarn run lint",
    "clean": "packemon clean",
    "docs": "cd website && yarn run start",
    "build": "beemo babel src/ --out-dir lib/",
    "build2": "packemon build --addEngines",
    "pack": "NODE_ENV=production packemon pack --addEngines --declaration=standard",
    "test": "beemo jest",
    "coverage": "yarn run test --coverage",
    "format": "beemo prettier",
    "lint": "beemo eslint",
    "type": "beemo typescript --build",
    "release": "yarn run pack && yarn run check && npx np --yolo"
  },
  "devDependencies": {
    "@beemo/cli": "^2.0.1",
    "@beemo/core": "^2.0.1",
    "@beemo/dev": "^1.4.0",
    "react": "^17.0.2"
  },
  "workspaces": [
    "packages/*",
    "website"
  ]
}
